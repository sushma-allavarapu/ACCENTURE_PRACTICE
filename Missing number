op:2

import java.util.*;
class Main{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int res=0;
        int a[]=new int[n-1];
        for(int i=0;i<n-1;i++)
        {
            a[i]=sc.nextInt();
        }
        int sum=(n*(n+1))/2;
        for(int i=0;i<n-1;i++)
        {
            res+=a[i];
        }
        System.out.println(sum-res);
    }
        
}
