import java.util.*;
class Main{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        String x=sc.next();
        String y=sc.next();
        int a=Integer.parseInt(x,2);
        int b=Integer.parseInt(y,2);
        int c=a+b;
        String z=Integer.toBinaryString(c);
        System.out.println(z);
    }
}
