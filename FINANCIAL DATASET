ip:7
op:7 6 5 4 3 2 1

import java.util.*;
class Main{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int c=0;
        int arr[]=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        for(int i=1;i<n;i++)
        {
            if(arr[i]<arr[i-1])
            {
                c+=1;
            }
        }
        System.out.println(c);
    }
}
