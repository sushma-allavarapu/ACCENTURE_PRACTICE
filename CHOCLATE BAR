op:5


import java.util.*;
class main{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int res=0;
        int n=sc.nextInt();
        int []arr=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        for(int i=0;i<n;i++)
        {
            res+=((arr[i]+2)/3);
        }
        System.out.println(res);
    
        
    }
}
