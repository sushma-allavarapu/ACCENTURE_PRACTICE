import java.util.*;
class Main{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        String st=sc.next();
        int k=sc.nextInt();
        for(int i=0;i<k;i++)
        {
            System.out.print(st.charAt(i));
        }
    }
}
