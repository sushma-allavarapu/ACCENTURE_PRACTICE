ip:rock
op:paper

import java.util.*;
import java.lang.*;
class main{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        String str=sc.nextLine();
        switch(str)
        {
            case "rock":
                System.out.println("paper");
                break;
            case "paper":
                System.out.println("scissors");
                break;
            case "scissors":
                System.out.println("rock");
                break;
            default:
                System.out.println("Invalid input");
                break;
        }
        
    }
}
